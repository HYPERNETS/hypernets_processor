[Processor]
archive_directory = 
network = w
version = 1.0
processor_working_directory = C:\Users\pdv\.hypernets
mcsteps = 10
max_level = L2A
bad_wavelenth_ranges = 757.5-767.5, 1350-1390
siteid=M1BE


[Databases]
metadata_db_url =
archive_db_url =
anomaly_db_url =

[Calibration]
hypstar_cal_number =
measurement_function_calibrate = StandardMeasurementFunction
clear_sky_check = True
calibration_directory =

[ModelName]
model = series_rep,series_id,vaa,azimuth_ref,vza,mode,action,it,scan_total,series_time

[Interpolate]
measurement_function_interpolate_time = InterpolationTimeLinear
measurement_function_interpolate_wav = InterpolationWavLinear
measurement_function_interpolate = LandNetworkInterpolationIrradianceLinear

[SurfaceReflectance]
measurement_function_surface_reflectance = WaterNetworkProtocol
measurement_function_water_leaving_radiance = WaterNetworkProtocolWaterLeavingRadiance

[WaterStandardProtocol]
verbosity = 3
protocol = water_std
n_upwelling_rad = 3
n_upwelling_irr = 3
n_downwelling_rad = 3

[Fresnelcorrection]
fresnel_option = Mobley
rhof_default = 0.0256
wind_ancillary = True
wind_default = 2.0
wind_file = False
wind_max_time_diff = 10
met_dir = /home/cgoyens/HYPSTAR/Ancillary/Wind_NCEP_MET/
rhymer_data_dir = ./rhymer/data
rholut = rhoTable_AO1999

[VariabilityCheck]
diff_test = True
diff_wave = 550
diff_threshold = 0.25
ed_cos_sza = True

[SimSpecSettings]
similarity_test = False
similarity_correct = True
similarity_wr = 670
similarity_wp = 0.05
similarity_w1 = 720
similarity_w2 = 780
similarity_alpha = 2.35

[WaterFinalMeasurementTest]
test_measurement = True
test_sun_wave = 750
test_sun_threshold = 0.05
test_var_wave = 780
test_var_threshold = 0.10

[Output]
product_format = netcdf
remove_vars_strings:
remove_vars_strings_L2:
write_l0: True
write_l1a: True
write_l1b: True
write_l1c: True
write_l2a: True

plotting_format: png
plot_l0: False
plot_l1a: False
plot_l1a_diff: False
plot_l1b: False
plot_l1c: False
plot_l2a: False
plot_uncertainty: False
plot_correlation: False
clear_sky_check=True

[CombineSWIR]
combine_lim_wav = 1000
measurement_function_combine = StepCombine

